{
  "name": "@cryptr/cryptr-react",
  "version": "1.3.7",
  "private": false,
  "dependencies": {
    "@cryptr/cryptr-spa-js": "^1.4.0",
    "ky": "^1.2.2"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  },
  "scripts": {
    "install:examples": "npm i --prefix=examples/react-javascript --no-package-lock && npm i --prefix=examples/react-typescript --no-package-lock && npm i --prefix=examples/communitiz-app-react-typescript --no-package-lock",
    "start:example:react-javascript": "npm start --prefix=examples/react-javascript",
    "start:example:react-typescript": "npm start --prefix=examples/react-typescript",
    "start:example:communitiz-app": "npm start --prefix=examples/communitiz-app-react-typescript",
    "lint": "eslint --ext=tsx ./src",
    "rollup:start": "rollup -cw",
    "publish-force": "npm unpublish --force && npm run build && ls && npm publish",
    "build": "npm run lint && rm -rf dist && rollup -c --environment NODE_ENV:production",
    "publish:lib": "npm run build && npm publish",
    "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|css|md)\"",
    "docs": "jsdoc -c jsdoc.conf.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "build:serve:react": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,ts,tsx"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/plugin-proposal-export-default-from": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^2.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.2",
    "react-router-dom": "^6.22.2",
    "rollup": "^4.12.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "web-vitals": "^3.5.2"
  },
  "description": "Cryptr React Authentication SDK",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.modern.js",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "cryptr",
    "react",
    "sdk",
    "authentication",
    "passwordless",
    "sso",
    "slo",
    "gateway"
  ],
  "author": "Cryptr (https://www.cryptr.co)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptr-auth/cryptr-react.git"
  },
  "bugs": "https://github.com/cryptr-auth/cryptr-react/issues",
  "homepage": "."
}
